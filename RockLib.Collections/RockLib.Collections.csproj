<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net451;net462</TargetFrameworks>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>RockLib.Collections</PackageId>
    <PackageVersion>1.0.0-alpha04</PackageVersion>
    <Authors>Quicken Loans</Authors>
    <Description>Various classes related to collections.</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes></PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/RockLib/RockLib.Collections</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/RockLib/RockLib.Collections/blob/master/LICENSE</PackageLicenseUrl>
    <Copyright>Copyright 2019 (c) Quicken Loans Corporation. All rights reserved.</Copyright>
    <PackageTags>rocklib collections</PackageTags>
    <Version>1.0.0</Version>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <PropertyGroup>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\RockLib.Collections.xml</DocumentationFile>
  </PropertyGroup>

</Project>
